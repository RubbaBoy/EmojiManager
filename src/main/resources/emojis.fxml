<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <VBox BorderPane.alignment="CENTER_LEFT">
            <children>
            <StackPane>
               <children>
                    <TextField maxWidth="1.7976931348623157E308" prefHeight="35.0" promptText="Search" StackPane.alignment="CENTER_LEFT" />
               </children>
                <VBox.margin>
                    <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
                </VBox.margin>
            </StackPane>
                <FlowPane maxHeight="25.0" prefHeight="25.0">
                    <children>
                        <CheckBox mnemonicParsing="false" text="Static">
                     <FlowPane.margin>
                        <Insets right="10.0" />
                     </FlowPane.margin></CheckBox>
                        <CheckBox mnemonicParsing="false" text="Animated">
                     <FlowPane.margin>
                        <Insets right="10.0" />
                     </FlowPane.margin></CheckBox>
                        <CheckBox mnemonicParsing="false" text="Regex">
                     <FlowPane.margin>
                        <Insets right="10.0" />
                     </FlowPane.margin></CheckBox>
                    </children>
                    <VBox.margin>
                        <Insets left="5.0" />
                    </VBox.margin>
                </FlowPane>
            </children>
        </VBox>
    </top>
   <bottom>
      <HBox prefHeight="50.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="uploadButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Upload">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="importButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Import">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="downloadButton" disable="true" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Download (0)">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="deleteButton" disable="true" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Delete (0)">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
         </children>
         <padding>
            <Insets left="7.0" top="10.0" />
         </padding>
      </HBox>
   </bottom>
   <center>
      <ScrollPane fitToWidth="true" hbarPolicy="NEVER" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets left="5.0" right="5.0" top="10.0" />
         </BorderPane.margin>
         <content>
            <FlowPane fx:id="emojiContent" hgap="10.0" minWidth="210.0" prefHeight="200.0" vgap="10.0" />
         </content>
      </ScrollPane>
   </center>
</BorderPane>
